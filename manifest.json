{
    "name":             "CueNation",
    "version":          "0.2.0",
    "manifest_version": 2,
    "description":      "Let you subscribe and track new cue files of your desired radio shows on CueNation.com.",
    "icons":            {
        "48": "images/icon48.png"
    },
    "browser_action":   {
        "default_icon":  "images/icon19.png",
        "default_title": "CueNation",
        "default_popup": "popup.html"
    },
    "background":       {
        "page": "background.html"
    },
    "permissions":      [
        "storage",
        "http://localhost:8080/*",
        "http://162.243.254.55:8080/*"
    ]
}
